<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <author>
    <name>Hendassa100k</name>
  </author>
  <title>$title$</title>
  <id>$url$/</id>
  <updated>$date$+00:00</updated>
  <link rel="self" href="$url$/atom.xml" />
  $for(posts)$
  <entry>
    <id>$posts.url$</id>
    <title>$posts.title$</title>
    <updated>$posts.date$T00:00:00+00:00</updated>
    <link href="$posts.url$" />
    <summary>$posts.abstract$</summary>
  </entry>
  $endfor$
</feed>
